While the citation process and this whole file is best dealt with by an external citation manager like Zotero, it can be helpful to understand what exactly is going on behind the scenes 

%articles are journals, magazines, or newspapers
@article{article,
author = {Du, Max and Astley, Richard},
journal = {Journal of Test},
pages = {167--174},
title = {{Text here will be displayed VerBaTiM}},
volume = {15},
year = {2020},
},

%for normal books 
@book{book,
author = {Du, Max},
title = {On the Effectiveness of LaTeX}, 
publisher = {Stanford University},
year = {2020},
},

%for "self-published" or otherwise non-publisher books 
@booklet{booklet, 
author = {Du, Max},
title = {On the Effectiveness of LaTeX, booklet edition}, 
year = {2020},
howpublished = {My Website},
},

%for parts of a book 
@inbook{inbook,
author = {Du, Max},
title = {small rabbit shiny light}, 
publisher = {Stanford University},
year = {2020},
chapter = {2--4},
},

%for parts of a book 
@inbook{inbookpages,
author = {Du, Max},
title = {correct horse battery stapler}, 
publisher = {Stanford University},
year = {2020},
pages = {20--42},
},

%for technical manuals 
@manual{manual, 
author = {Du, Max},
title = {Using LaTeX: a comprehensive manual}, 
year = {2020},
organization = {Stanford University}
},

%for conferences 
@inproceedings{proceedings,
author = {Gabbay, Dov and Pnueli, Amir and Shelah, Saharon and Stavi,
 Jonathan},
booktitle = {Proceedings of the 7th ACM SIGPLAN-SIGACT Symposium on
 Principles of Programming Languages},
pages = {163--173},
publisher = {Association Comp. Machinery, NY},
series = {POPL '80},
title = {{On the Temporal Analysis of Fairness}},
year = {1980},
},

%for thesis writing  
@mastersthesis{thesis, 
author = {Du, Max},
title = {Using LaTeX: a comprehensive manual}, 
year = {2020},
school = {Stanford University}
},

%for everything else 
@misc{misc, 
author = {Du, Max},
title = {Random random}, 
year = {2020},
howpublished = {Leaflet}
},

%for online websites (not news articles!). You can add the accession date 
@online{online, 
author = {Du, Max},
title = {richard rotation}, 
year = {2020},
url = {www.google.com},
urldate = {2020-08-23},
notes = {aasdfasdfasdfasdfasf},
},

%see the cheat sheet for more details. 



